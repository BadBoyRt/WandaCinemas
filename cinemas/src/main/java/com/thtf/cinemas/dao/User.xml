<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thtf.cinemas.dao.UserDao">
	<resultMap id="BaseResultMap" type="com.thtf.cinemas.domain.User">
		<id column="id" javaType="BIGINT" property="id"/>
		<result column="username" javaType="VARCHAR" property="username"/>
		<result column="password" javaType="VARCHAR" property="password"/>
	</resultMap>
	<select id="selectAllUser" resultMap="BaseResultMap">
		select * from user
	</select>
</mapper>